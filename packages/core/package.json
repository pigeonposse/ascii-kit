{
	"name": "ascii-kit",
	"version": "0.0.4",
	"description": "Toolkit for create awesome ascii art",
	"keywords": [
		"ascii",
		"ascii-art",
		"ascii-kit",
		"dovenv",
		"pigeonposse",
		"pp"
	],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/main.d.mts",
				"default": "./dist/main.mjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"test": "vitest run -r src --passWithNoTests"
	},
	"dependencies": {
		"@ascii-kit/font": "workspace:*",
		"@ascii-kit/image": "workspace:*",
		"@ascii-kit/qr": "workspace:*",
		"@ascii-kit/svg": "workspace:*",
		"@ascii-kit/tree": "workspace:*"
	},
	"devDependencies": {
		"@ascii-kit/repo-config": "workspace:*"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
